{
  "manifest_version": 2,
  "name": "Delete workflow run in GitHub",
  "version": "0.0.0.1",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png",
    "512": "icons/512.png"
  },
  "permissions": [
    "tabs",
    "<all_urls>"
  ],
  "browser_action": {
    "default_icon": "icons/16.png",
    "default_popup": "popup.html"
  }
}